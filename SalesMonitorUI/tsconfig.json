{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost"],
      "jsx": "react-jsx",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": ["src"]
  }
  